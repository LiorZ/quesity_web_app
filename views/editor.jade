html(lang="en")
	head
		title The Questructor!
		script(data-main='/editor/js/boot', type='text/javascript',src='http://maps.google.com/maps/api/js?sensor=true&key=AIzaSyCI3y0xsOOIUye7jrp6HWjnOl1OTrCY4ls')
		script(type='text/javascript',src='/shared/js/lib/require/require.js')
		link(rel='stylesheet',type='text/css', href='/shared/js/lib/jquery-ui/css/smoothness/jquery-ui-1.10.0.custom.css')
		link(rel='stylesheet',type='text/css', href='/shared/js/lib/jquery-ui/plugins/tablesorter/themes/blue/style.css')
		link(rel='stylesheet', type='text/css', href='/editor/css/editor.css')
		link(rel='stylesheet', type='text/css', href='/editor/css/editor.css')
		link(rel='stylesheet', type='text/css', href='/shared/js/lib/jquery-ui/plugins/select2/select2.css')
		link(rel='stylesheet', type='text/css', href='/shared/js/lib/jquery-ui/plugins/tagit/jquery.tagit.css')
	body
		input#quest_id(type='hidden', value=quest_id)
		div#container
			div#menu
				button#btn_settings Settings
				button#btn_new_page New Page
				button Demo
				button Publish
				button#btn_exit_editor Exit Editor
				button#menu_opener.smaller_fonts &gt; &gt;
				ul#new_page_menu.smaller_fonts
					li 
						a(href="#show_diagram", data-page-type="location") Location
					li 
						a(href="#show_diagram", data-page-type="question") Question
					li 
						a(href="#show_diagram", data-page-type="open_question") Open Question
					li 
						a(href="#show_diagram", data-page-type="static") Static
					li 
						a(href="#show_diagram", data-page-type="surprise") Surprise
					li 
						a(href="#show_diagram", data-page-type="stall") Stall
			div#toolbar.ui-widget-header.ui-corner-all
				button#btn_edit.smaller_fonts Edit Page
				button#btn_delete.smaller_fonts Delete Page
			div#world